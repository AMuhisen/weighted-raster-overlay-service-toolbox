<?xml version="1.0" encoding="utf-8"?>
<Application>
  <Name>LandscapeModeler</Name>
  <Command>CM+AR+CC+BB+SP+ERF1+ERF</Command>
  <ArcGISVersion>
	<Product>
		<Min>10.3.0.3348</Min>

	</Product>
  </ArcGISVersion>

  <Workspace>
    <WorkspacePath>MD</WorkspacePath>
    <Geodatabase>LSM</Geodatabase>
      <MosaicDataset>
      <MosaicDatasetType>Source</MosaicDatasetType>
        <Name>S_LSM_MD</Name>
      <SRS>PROJCS['WGS_1984_Web_Mercator_Auxiliary_Sphere',GEOGCS['GCS_WGS_1984',DATUM['D_WGS_1984',SPHEROID['WGS_1984',6378137.0,298.257223563]],PRIMEM['Greenwich',0.0],UNIT['Degree',0.0174532925199433]],PROJECTION['Mercator_Auxiliary_Sphere'],PARAMETER['False_Easting',0.0],PARAMETER['False_Northing',0.0],PARAMETER['Central_Meridian',0.0],PARAMETER['Standard_Parallel_1',0.0],PARAMETER['Auxiliary_Sphere_Type',0.0],UNIT['Meter',1.0]];-20037700 -30241100 10000;-100000 10000;-100000 10000;0.001;0.001;0.001;IsHighPrecision</SRS>
      <num_bands>1</num_bands>
	  <pixel_type>32_BIT_FLOAT</pixel_type>
      <AddRasters> 
         <AddRaster>	   
          <dataset_id>LSM</dataset_id>
          <raster_type>Table</raster_type>
          <Sources>
            <data_path>r:\Test\Data</data_path>
          </Sources>
          
	        <update_cellsize_ranges>UPDATE_CELL_SIZES</update_cellsize_ranges>
          <update_boundary>#</update_boundary>
         </AddRaster>
        </AddRasters> 
        <DefaultProperties>
          <max_num_per_mosaic>50</max_num_per_mosaic>
          <rows_maximum_imagesize>4000</rows_maximum_imagesize>
          <columns_maximum_imagesize>4000</columns_maximum_imagesize>
          <allowed_compressions>LZ77;NONE;LERC;JPEG</allowed_compressions>
          <default_compression_type>LZ77</default_compression_type>
          <JPEG_quality>85</JPEG_quality>
	  <resampling_type>Nearest</resampling_type>
	  <LERC_Tolerance>0.01</LERC_Tolerance>
          <clip_to_footprints>CLIP</clip_to_footprints>
          <clip_to_boundary>CLIP</clip_to_boundary>
	  <color_correction>NOT_APPLY</color_correction>
	  <footprints_may_contain_nodata>FOOTPRINTS_MAY_CONTAIN_NODATA</footprints_may_contain_nodata>
	  <allowed_mensuration_capabilities>BASIC</allowed_mensuration_capabilities>
	  <default_mensuration_capabilities>BASIC</default_mensuration_capabilities>
	  <allowed_mosaic_methods>None;ByAttribute;Seamline;NorthWest;Center;LockRaster;Nadir;Viewpoint</allowed_mosaic_methods>
          <default_mosaic_method>None</default_mosaic_method>
          <Order_field>#</Order_field>  
	  <order_base>#</order_base>
	  <sorting_order>Ascending</sorting_order>
	  <mosaic_operator>FIRST</mosaic_operator>
	  <blend_width>0</blend_width>
	  <view_point_x>300</view_point_x>
	  <view_point_y>300</view_point_y>
	  <cell_size_tolerance>0.8</cell_size_tolerance>
	  <cell_size>#</cell_size>
	  <metadata_level>BASIC</metadata_level>
      <transmission_fields>Name;MinPS;MaxPS;LowPS;HighPS;ProductName;Title;Url;Description;Metadata;MinMaxRange;DisplayOrder;InputRanges;OutputValues;RangeLabels;NoDataRanges;NoDataRangeLabels</transmission_fields>
	  <use_time>#</use_time>
	  <start_time_field>#</start_time_field>
	  <end_time_field>#</end_time_field>
	  <time_format>#</time_format>
	  <geographic_transform>#</geographic_transform>
	  <max_num_of_download_items>50</max_num_of_download_items>
	  <max_num_of_records_returned>2000</max_num_of_records_returned>
        </DefaultProperties>
         <Table>
          <AddFields>AddFields</AddFields>
          <Fields>
            <Field>
              <Name>Dataset_ID</Name>
              <Type>TEXT</Type>
              <Length>32</Length>
            </Field>
			<Field>
              <Name>Raster</Name>
              <Type>TEXT</Type>
              <Length>255</Length>
            </Field>
			<Field>
              <Name>Name</Name>
              <Type>TEXT</Type>
              <Length>100</Length>
            </Field>
			<Field>
              <Name>Title</Name>
              <Type>TEXT</Type>
              <Length>50</Length>
            </Field>
			<Field>
              <Name>Url</Name>
              <Type>TEXT</Type>
              <Length>1024</Length>
            </Field>
			<Field>
              <Name>Description</Name>
              <Type>TEXT</Type>
              <Length>255</Length>
            </Field>
			<Field>
              <Name>Metadata</Name>
              <Type>TEXT</Type>
              <Length>255</Length>
            </Field>
			<Field>
              <Name>InputRanges</Name>
              <Type>TEXT</Type>
              <Length>256</Length>
            </Field>
			<Field>
              <Name>OutputValues</Name>
              <Type>TEXT</Type>
              <Length>256</Length>
            </Field>
			<Field>
              <Name>NoDataRanges</Name>
              <Type>TEXT</Type>
              <Length>256</Length>
            </Field>
			<Field>
              <Name>RangeLabels</Name>
              <Type>TEXT</Type>
              <Length>1024</Length>
            </Field>
			<Field>
              <Name>NoDataRangeLabels</Name>
              <Type>TEXT</Type>
              <Length>1024</Length>
            </Field>
          </Fields>
         </Table>
	  <Processes>
		<CalculateCellSizeRanges>
			<where_clause></where_clause>
			<do_compute_min>NO_MIN_CELL_SIZES</do_compute_min>
			<do_compute_max>NO_MAX_CELL_SIZES</do_compute_max>
			<max_range_factor>10</max_range_factor>
			<cell_size_tolerance_factor>0.8</cell_size_tolerance_factor>
			<update_missing_only>UPDATE_ALL</update_missing_only>
		</CalculateCellSizeRanges>
       
        <BuildBoundary>
			<simplification_method>ENVELOPE</simplification_method>
		</BuildBoundary>
       
        <BuildOverviews>

			<generate_overviews>GENERATE_OVERVIEWS</generate_overviews>
		</BuildOverviews>		
        <DefineOverviews>
			<pixel_size>80</pixel_size>
			<overview_factor>2</overview_factor>
			<resampling_method>BILINEAR</resampling_method>
			<compression_method>JPEG</compression_method>
			<compression_quality>85</compression_quality>
		</DefineOverviews>
   <EditRasterFunction>
			<edit_mosaic_dataset_item>#</edit_mosaic_dataset_item>
			<edit_options>INSERT</edit_options>
			<function_chain_definition>Keymetadata.rft.xml</function_chain_definition>
      <location_function_name>#</location_function_name>

		</EditRasterFunction>
		<EditRasterFunction>
			<edit_mosaic_dataset_item>#</edit_mosaic_dataset_item>
			<edit_options>Remove</edit_options>
			<function_chain_definition>Keymetadata.rft.xml</function_chain_definition>
      <location_function_name>#</location_function_name>

		</EditRasterFunction>
        <CalculateValues>

         <CalculateValue>
		  <FieldName>InputRanges</FieldName>
 		  <Expression>1,21,21,41,41,61,51,81,81,101</Expression>
	  </CalculateValue>
         <CalculateValue>
		  <FieldName>OutputValues</FieldName>
		  <Expression>1,3,5,7,9</Expression>
	  </CalculateValue>
	  <CalculateValue>
		  <FieldName>NoDataRanges</FieldName>
		  <Expression>0,0</Expression>
	  </CalculateValue>
	  <CalculateValue>
		  <FieldName>RangeLabels</FieldName>
		  <Expression>Very Poor,Poor,Average,Good,Excellent</Expression>
	  </CalculateValue>
	<CalculateValue>
		  <FieldName>NoDataRangeLabels</FieldName>
		  <Expression>No Data</Expression>
	  </CalculateValue>
	</CalculateValues>
	<SetStatistics>
			<data_type>Generic</data_type>
			<stats_file>stats.xml</stats_file>
	</SetStatistics>
       </Processes>
	
      </MosaicDataset>
  </Workspace>
</Application>
